source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'

target 'TumblrFeedApp' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for Tumblr
    pod 'AlamofireImage'
    
    target 'TumblrFeedAppTests' do
        inherit! :search_paths
        # Pods for testing
    end
    
    target 'TumblrFeedAppUITests' do
        inherit! :search_paths
        # Pods for testing
    end
    
end
